{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Client",
      "type": "blazorwasm",
      "request": "launch",
      "cwd": "${workspaceFolder}/Client"
    },
    {
      "name": "Attach to .NET Functions",
      "type": "coreclr",
      "request": "attach",
      "processId": "${command:azureFunctions.pickProcess}"
    }
  ],
  "compounds": [
    {
      "name": "Launch",
      "configurations": ["Attach to .NET Functions", "Launch Client"]
    }
  ]
}
